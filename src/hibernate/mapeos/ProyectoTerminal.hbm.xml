<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="hibernate.beans.ProyectoTerminal" table="proyecto_terminal">
		<id name="idProyectoTerminal" column="id_proyecto_terminal" type="int" />
		<property name="titulo" column="titulo" type="string" />
		<property name="cantitadAlumnos" column="cantitad_alumnos" type="int" />
		<property name="fechaInicio" column="fecha_inicio" type="date" />
		<property name="fechaFinalizacion" column="fecha_finalizacion" type="date" />
		<property name="puntos" column="puntos" type="int" />
		<property name="estado" column="estado" type="string" />
		<property name="proyectoInvestigacionIdProyectoInvestigacion" column="proyecto_investigacion_id_proyecto_investigacion" type="string" />
	</class>
</hibernate-mapping>